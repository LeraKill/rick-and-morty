{
  "root": true,
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "react-hooks/exhaustive-deps": "off",
    "react-hooks/rules-of-hooks": "off",
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    //    "indent": [ "error", 2, {"SwitchCase":  1} ],
    "space-before-function-paren": "off",
    "jsx-quotes": ["error", "prefer-double"],
    "no-console": "warn"
  }
}
